<h1>Editing Capitulo</h1>

    <%= simple_form_for(@capitulo) do |f| %>


          <%= f.input :title %>
          <%= f.input :episode %>
          <%= f.input :season %>
          <%= f.association :serie %>
          <%= f.input :description%>

          <%= f.button :submit %>

    <% end %>

<%= link_to 'Show', @capitulo %> |
<%= link_to 'Back', capitulos_path %>
